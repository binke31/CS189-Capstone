<div data-role="content">
        <div data-role="collapsible-set" data-theme="b" data-content-theme="d">
          <% if matchesRentPath?( (request.referer ? URI(request.referer).path : "") ) %>
            <div data-role="collapsible" data-collapsed="false">
          <% else %>
            <div data-role="collapsible">
          <% end %>
                <h3>
                    Rent
                </h3>
                <ul data-role="listview" data-divider-theme="b" data-inset="false">
				  <li data-theme="d">
				    <a href="/home/my_info" data-transition="slide">
				      My Info
                    </a>
				  </li>
				  <li data-theme="d">
                    <a href="/home/pay_rent" data-transition="slide">
                      Submit Payment
                    </a>
                  </li>
                  <li data-theme="d">
                     <a href="/home/payment_history" data-transition="slide">
                      Payment History
                     </a>
                  </li>
                </ul>
            </div>
          <% if matchesMaintenancePath?( (request.referer ? URI(request.referer).path : "") ) %>
            <div data-role="collapsible" data-collapsed="false">
          <% else %>
            <div data-role="collapsible">
          <% end %>
                <h3>
                    Maintenance
                </h3>
                <ul data-role="listview" data-divider-theme="b" data-inset="false">
                    <li data-theme="c">
                        <a href="/home/maintenance_request" data-transition="slide">New Maintenance Request</a>
                    </li>
                    <li data-theme="c">
                        <a href="/home/maintenance_log" data-transition="slide">
                            Maintenance Log
                        </a>
                    </li>
                </ul>
            </div>
          <% if matchesNotificationPath?( (request.referer ? URI(request.referer).path : "") ) %>
            <div data-role="collapsible" data-collapsed="false">
          <% else %>
            <div data-role="collapsible">
          <% end %>
                <h3>
                    Notifications
                </h3>
                <ul data-role="listview" data-divider-theme="b" data-inset="false">
                    <li data-theme="c">
                        <a href="/home/notifications" data-transition="slide">
                            View Notifications
                            <span class="ui-li-count">
                                ###
                            </span>
                        </a>
                    </li>
										<li data-theme="c">
											<a href="/settings/notification_settings" data-transition="slide">
												Adjust Settings
											</a>
										</li>
                </ul>
            </div>
          <% if matchesOtherPath?( (request.referer ? URI(request.referer).path : "") ) %>
            <div data-role="collapsible" data-collapsed="false">
          <% else %>
            <div data-role="collapsible">
          <% end %>
                <h3>
                    Other
                </h3>
                <ul data-role="listview" data-divider-theme="b" data-inset="false">
                    <li data-theme="c">
                        <a href="#page8" data-transition="slide">
                            Contact Property Manager
                        </a>
                    </li>
                </ul>
            </div>
        </div>
</div>